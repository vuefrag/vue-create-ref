  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuefrag/vue-create-ref.git"
  },
  "bugs": {
    "url": "https://github.com/vuefrag/vue-create-ref/issues"
  },
  "homepage": "https://github.com/vuefrag/vue-create-ref#readme",
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "unbuild": "^2.0.0",
